{
  "name": "v6",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "ng serve",
    "build": "ng build",
    "preview": "ng serve --configuration=production",
    "cap:init": "npx cap init MotionSignal com.motionsignal.app --web-dir=dist",
    "cap:add:android": "npx cap add android",
    "cap:sync": "npx cap sync android",
    "open:android": "npx cap open android",
    "build:android": "ng build && npx cap sync android",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:race": "vitest --run --testNamePattern='race condition'",
    "test:perf": "vitest --run --testNamePattern='performance'",
    "test:ci": "vitest --run --coverage"
  },
  "dependencies": {
    "@angular/build": "^20.3.9",
    "@angular/cli": "^20.3.9",
    "@angular/common": "^20.3.10",
    "@angular/compiler": "^20.3.10",
    "@angular/compiler-cli": "^20.3.10",
    "@angular/core": "^20.3.10",
    "@angular/platform-browser": "^20.3.10",
    "@capacitor-community/bluetooth-le": "^7.2.0",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/core": "^7.4.4",
    "@mediapipe/pose": "^0.5.1675469404",
    "@mediapipe/tasks-vision": "^0.10.22-rc.20250304",
    "@tensorflow-models/pose-detection": "^2.1.3",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@tensorflow/tfjs-backend-webgpu": "^4.22.0",
    "@tensorflow/tfjs-converter": "^4.22.0",
    "@tensorflow/tfjs-core": "^4.22.0",
    "diffyjs": "^2.0.0",
    "firebase": "^12.5.0",
    "rxjs": "^7.8.2",
    "speedy-vision": "^0.9.0-wip",
    "tailwindcss": "latest"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.9",
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@testing-library/angular": "^18.1.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.0",
    "@vitest/ui": "^4.0.8",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.1.0",
    "typescript": "~5.8.3",
    "vite": "^6.4.1",
    "vitest": "^4.0.8"
  }
}
